{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "allowJs": false,
    "jsx": "react-jsx",
  "types": ["expo", "node", "jest"],
    "baseUrl": ".",
    "paths": {
      "lib/*": ["lib/*"],
      "components/*": ["components/*"],
      "features/*": ["features/*"],
      "app/*": ["app/*"],
  "tamagui.config": ["tamagui.config.ts"],
      "sentry": ["sentry.ts"]
    }
  },
  "include": [
    "*.ts",
    "*.tsx",
    "app/**/*.ts",
    "app/**/*.tsx",
    "components/**/*.ts",
    "components/**/*.tsx",
    "features/**/*.ts",
    "features/**/*.tsx",
    "lib/**/*.ts",
  "lib/**/*.tsx",
  "__tests__/**/*.ts",
  "__tests__/**/*.tsx",
    "types/**/*.ts",
    "scripts/**/*.ts",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ],
  "exclude": [
    "node_modules",
    "build",
    "dist",
    "exports",
    "android",
    "ios",
    "vendor"
  ]
}
